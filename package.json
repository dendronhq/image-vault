{
	"name": "imagevault",
	"displayName": "Image Vault",
	"description": "Store and serve images without bloating your git repo",
	"version": "0.0.2",
	"publisher": "dendronhq",
	"repository": "https://github.com/dendronhq/image-vault",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:markdown"
	],
	"main": "./out/src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.pasteImage",
				"title": "Image Vault: Paste Image"
			},
			{
				"command": "extension.storeSecret",
				"title": "Image Vault: Store AWS Secret Key"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Image Vault Configuration",
			"properties": {
				"imageVault.accessKeyId": {
					"type": "string",
					"description": "AWS Access Key ID"
				},
				"imageVault.region": {
					"type": "string",
					"description": "AWS Region"
				},
				"imageVault.bucketEndpoint": {
					"type": "string",
					"description": "AWS Bucket Endpoint"
				},
				"imageVault.bucketName": {
					"type": "string",
					"description": "AWS Bucket Name"
				},
				"imageVault.bucketPrefix": {
					"type": "string",
					"description": "AWS Bucket Prefix"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"package": "vsce package",
		"compile": "tsc -p ./",
		"install": "code-insiders --install-extension",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.195",
		"@types/mime": "^3.0.1",
		"@types/mocha": "^10.0.1",
		"@types/moment": "^2.13.0",
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^5.0.2"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.350.0",
		"lodash": "^4.17.21",
		"mime": "^3.0.0",
		"moment": "^2.29.4"
	}
}
