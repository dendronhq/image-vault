{
	"version": "0.2.0",
	"configurations": [{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"env": {
				"NODE_ENV": "development"
			},
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}",
				"--disable-extensions",
			],
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			]
		}
	],
  "preLaunchTask": "npm: watch"
}
